{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./src",
  "entryFile": "infra/http/nest/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "typeCheck": true,
    "builder": {
      "type": "swc",
      "options": {
        "swcrcPath": "./.swcrc"
      }
    }
  }
}
